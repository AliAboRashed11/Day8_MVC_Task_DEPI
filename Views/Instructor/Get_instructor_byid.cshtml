@model Instructor


@{
    Instructor ins = Model;
}


<main class="container ">

    @if (TempData["message"] != null)
    {

        <div class="alert alert-info text-center mb-4 fw-bolder">
            @TempData["message"]
        </div>
    }
    else
    {
        <div class="alert alert-info text-center  fw-bolder">
            Welcome To @ins.Name Page !
        </div>

    }

    <div class="row mb-6">



        <partial name="_InstructorPartial" model="ins"></partial>

    </div>




</main>